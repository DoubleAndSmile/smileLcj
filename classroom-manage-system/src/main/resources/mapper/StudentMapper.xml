<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iflytek.classroommanagesystem.mapper.StudentMapper">

    <resultMap id="BaseResultMap" type="com.iflytek.classroommanagesystem.entity.Student">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="studentNo" column="student_no" jdbcType="CHAR"/>
            <result property="name" column="name" jdbcType="CHAR"/>
            <result property="gender" column="gender" jdbcType="CHAR"/>
            <result property="birthday" column="birthday" jdbcType="DATE"/>
            <result property="gradeId" column="grade_id" jdbcType="INTEGER"/>
            <result property="professionId" column="profession_id" jdbcType="INTEGER"/>
            <result property="classId" column="class_id" jdbcType="INTEGER"/>
            <result property="password" column="password" jdbcType="CHAR"/>
            <result property="phone" column="phone" jdbcType="CHAR"/>
            <result property="email" column="email" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,student_no,name,
        gender,birthday,grade_id,
        profession_id,class_id,password,
        phone,email
    </sql>
</mapper>
